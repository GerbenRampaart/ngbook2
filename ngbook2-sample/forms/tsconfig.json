{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "filesGlob": [
    "./**/*.ts",
    "./test/**/*.ts",
    "!./node_modules/**/*.ts"
  ],
  "files": [
    "node_modules/angular2/ts/typings/node/node.d.ts",
    "node_modules/angular2/typings/browser.d.ts",
    "./typings/tsd.d.ts",
    "./app/ts/app.ts",
    "./app/ts/forms/demo_form_ng_model.ts",
    "./app/ts/forms/demo_form_sku_with_builder.ts",
    "./app/ts/forms/demo_form_sku.ts",
    "./app/ts/forms/demo_form_with_custom_validations.ts",
    "./app/ts/forms/demo_form_with_events.ts",
    "./app/ts/forms/demo_form_with_validations_explicit.ts",
    "./app/ts/forms/demo_form_with_validations_shorthand.ts",
    "./test/forms/demo_form_ng_model.spec.ts",
    "./test/forms/demo_form_sku_with_builder.spec.ts",
    "./test/forms/demo_form_sku.spec.ts",
    "./test/forms/demo_form_with_custom_validations.spec.ts",
    "./test/forms/demo_form_with_events_bad.spec.ts",
    "./test/forms/demo_form_with_events.spec.ts",
    "./test/forms/demo_form_with_validations_explicit.spec.ts",
    "./test/forms/demo_form_with_validations_shorthand.spec.ts",
    "./vendor.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": true
  }
}
